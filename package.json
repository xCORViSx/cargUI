    "menus": {
      "view/title": [
        {
          "command": "cargui.new",
          "when": "view == cargoTargets",
          "group": "navigation@1"
        },
        {
          "command": "cargui.build",
          "when": "view == cargoTargets",
          "group": "navigation@2"
        },
